{
  "name": "webgen-phase1",
  "version": "2.0.0",
  "description": "WebGen Phase 3 - Static Site Generator with index.html support",
  "main": "_buildr/generator.js",
  "scripts": {
    "cms-watcher": "node _buildr/watcher.js",
    "cms-regen-page": "node _buildr/generator.js --incremental",
